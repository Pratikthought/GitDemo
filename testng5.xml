<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests" thread-count="2">
<listeners>
<listener class-name="packageName.Listeners"></listener>
</listeners>
<parameter name="URL" value="QaAcademy.com.com"/>
	<test name="Personal Loan">
	<parameter name="URL" value="personalloan.com"/>
		<classes>
			<class name="packageName.Day1">
				<methods>
					<include name="Two" />
				</methods>
			</class>
			<class name="packageName.Day2" />
			
		</classes>

	</test> <!-- Test -->


	<test name="Car Loan">
	<parameter name="URL" value="carloan.com"/> 
		<classes>
			<class name="packageName.Day3">
				<methods>
					<exclude name="API.*" />
				</methods>
			</class>
		</classes>
	</test>
	
	<test name="Home Loan">
		<classes>
			<class name="packageName.Day4">
			</class>
		</classes>
	</test>
	
</suite> <!-- Suite -->
